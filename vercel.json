{
  "builds": [
    { "src": "*.tsx", "use": "@vercel/node" },
    { "src": "*.ts", "use": "@vercel/node" },
    { "src": "*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/*", "dest": "api_server/api.py" },
    { "src": "/api/teamkill-board/:outfit_id", "dest": "api_server/api.py" }
  ]
}
